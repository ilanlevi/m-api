{
  "name": "mid-api",
  "version": "1.0.0",
  "description": "graphql redis connection client",
  "main": "src/main.ts",
  "scripts": {
    "debug": "npm run build && node --inspect-brk dist/main.js",
    "clean": "rimraf dist",
    "start": "node dist/main.js",
    "generate": "graphql-codegen",
    "prebuild": "npm run clean && npm run generate",
    "build": "tsc && npm run prettier:all",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --write",
    "prettier:all": "prettier --write \"src/**/*.ts\""
  },
  "author": "ITK",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/typescript": "1.13.5",
    "@types/express": "4.17.2",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "graphql-playground-react": "1.7.20",
    "prettier": "1.19.1",
    "typescript": "3.7.5",
    "nodemon": "2.0.3"
  },
  "dependencies": {
    "apollo-server": "2.9.16",
    "apollo-server-express": "^2.9.16",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "14.6.0",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.1",
    "winston-udp": "0.0.7",
    "ioredis": "^4.16.3",
    "fartsampler": "^0.1.6"
  },
  "_moduleAliases": {
    "src": "dist"
  }
}
